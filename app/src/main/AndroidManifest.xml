<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.applabarra">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/isotipo"
        android:label="La Barra"
        android:roundIcon="@drawable/isotipo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppLaBarra"
        tools:targetApi="31">

        <activity
            android:name=".menu.MenuActivity"
            android:exported="false" />

        <activity
            android:name=".RegistroActivity"
            android:exported="false" />

        <activity
            android:name=".LoginActivity"
            android:exported="false" />

        <activity
            android:name=".PagPrincipal"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.example.applabarra.menu.NosotrosActivity"
            android:exported="false" />

    </application>
</manifest>


